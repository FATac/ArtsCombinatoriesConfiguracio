	            {"exp":"author_role='contractor'", "result":{"block":"bOrder"}},
	            {"exp":"author_role='independent' || author_role='curator' ", "result":{"block":"bAssignment"}},
	            {"exp":"author_role='employed' || author_role='other' ", "result":{"block":"bWorkType"}}
